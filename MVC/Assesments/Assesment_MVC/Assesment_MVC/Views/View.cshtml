@model IEnumerable<Assesment_MVC.Models.Customer>

<h2>CustomersList</h2>
<table>
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>Contact Name</th>
        <th>City</th>
    </tr>
    <tbody>
        @foreach (var customer in Model)
        {
<tr>
    <td>@customer.CustomerID</td>
    <td>@customer.CompanyName</td>
    <td>@customer.ContactName</td>
    <td>@customer.City</td>
</tr>
}
    </tbody>
</table>